{
  "name": "kintone-as-code",
  "version": "0.4.0",
  "description": "A CLI tool for managing kintone applications as code with type-safe TypeScript schemas",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "kintone-as-code": "./dist/cli.js"
  },
  "type": "module",
  "scripts": {
    "build": "tsc",
    "dev": "tsx watch src/cli.ts",
    "test": "vitest",
    "test:unit": "vitest run src/core src/commands",
    "test:integration": "npm run build && vitest run src/__tests__/cli.integration.test.ts",
    "test:all": "npm run test:unit && npm run test:integration",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src --ext ts",
    "lint:fix": "eslint src --ext ts --fix",
    "format": "prettier --write src/**/*.ts",
    "format:check": "prettier --check src/**/*.ts",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build && npm run test:all"
  },
  "keywords": [
    "kintone",
    "cybozu",
    "infrastructure-as-code",
    "cli",
    "typescript",
    "effect"
  ],
  "author": "kimotokensei",
  "license": "MIT",
  "dependencies": {
    "@kintone/rest-api-client": "^5.0.0",
    "chalk": "^5.3.0",
    "commander": "^11.0.0",
    "dotenv": "^16.3.1",
    "effect": "^3.0.0",
    "esbuild": "^0.19.0",
    "kintone-effect-schema": "^0.8.0",
    "ora": "^6.3.0",
    "yargs": "^18.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/yargs": "^17.0.33",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitest/coverage-v8": "^1.6.1",
    "eslint": "^8.0.0",
    "prettier": "^3.0.0",
    "tsx": "^4.0.0",
    "typescript": "^5.0.0",
    "vitest": "^1.0.0"
  },
  "files": [
    "dist",
    "README.md",
    "README.ja.md",
    "LICENSE"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Kensei-Kimoto/kintone-as-code.git"
  },
  "bugs": {
    "url": "https://github.com/Kensei-Kimoto/kintone-as-code/issues"
  },
  "homepage": "https://github.com/Kensei-Kimoto/kintone-as-code#readme"
}
